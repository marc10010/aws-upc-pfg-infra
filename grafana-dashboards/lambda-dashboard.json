{
  "id": null,
  "title": "Lambda Functions Monitoring",
    "tags": ["lambda", "aws"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Lambda Invocations",
        "type": "graph",
